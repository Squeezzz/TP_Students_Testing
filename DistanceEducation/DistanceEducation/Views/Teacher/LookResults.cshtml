@{
    List<Student> students = ViewData["Students"] != null ? (List<Student>)ViewData["Students"] : null;
    List<Result> results = ViewData["Results"] != null ? (List<Result>)ViewData["Results"] : null;
    int maxpoint = Convert.ToInt32(ViewData["Question"]);
}

<div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Студент
                </th>
                <th>
                    Результат
                </th>
            </tr>
        </thead>
        @foreach(var item in students)
        {
            <td>
                @item.Surname @item.Name @item.Patronymic
            </td>
            <td>
                @results.Where(a=>a.StudentId==item.Id).Select(a=>a.Itog).FirstOrDefault() / @maxpoint
            </td>
        }
    </table>

    <hr />
    <a asp-action="MainPageTeacher">Вернуться на главную</a>
    <hr />
    <a asp-action="TestList">Вернуться к списку тестов</a>
</div>